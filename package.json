{
    "$schema": "http://json.schemastore.org/package",
    "name": "eo-certificate-backend",
    "version": "1.0.0",
    "type": "commonjs",
    "description": "Backend API for EO Certificate Management System with PostgreSQL",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "db:migrate": "knex migrate:latest",
      "db:rollback": "knex migrate:rollback",
      "db:seed": "knex seed:run",
      "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
      "db:make-migration": "knex migrate:make",
      "db:make-seed": "knex seed:make",
      "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
      "express": "^4.18.2",
      "knex": "^2.5.1",
      "pg": "^8.11.3",
      "cors": "^2.8.5",
      "bcryptjs": "^2.4.3",
      "jsonwebtoken": "^9.0.2",
      "multer": "^1.4.5-lts.1",
      "xlsx": "^0.18.5",
      "dotenv": "^16.3.1"
    },
    "devDependencies": {
      "nodemon": "^3.0.1"
    },
    "keywords": [
      "eo-certificate",
      "backend",
      "api",
      "postgresql",
      "knex",
      "express"
    ],
    "author": "Your Name",
    "license": "ISC",
    "engines": {
      "node": ">=14.0.0"
    },
    "private": true
  }